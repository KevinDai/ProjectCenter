    <script type="text/javascript">
        var filterLinkViewModel = function (field, name, selectListItems, defaultValue) {
            var self = this;
            self.field = field;
            self.name = name;
            self.selectListItems = selectListItems;
            self.value = ko.observable(defaultValue);
            self.select = function (data) {
                self.value(data.value);
            };
            self.clear = function () {
                self.value("");
            }
        };

        var pageListViewModel = function (pageList) {
            var self = this;
            self.data = pageList.List;
            self.pageIndex = pageList.PageIndex;
            self.pageSize = pageList.PageSize;
            self.total = pageList.Total;
            self.maxPageIndex = self.total / self.pageSize;
            self.startIndex = (self.pageIndex - 1) * self.pageSize + 1;
            self.endIndex = self.pageIndex * self.pageSize > self.total ? self.total : self.pageIndex * self.pageSize;
        };

    </script>
